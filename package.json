{
  "name": "pandita",
  "version": "1.0.0",
  "description": "A modern, interactive coaching and training institute website built with HTML and Tailwind CSS.",
  "main": "index.js",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./src/input.css -o ./assets/css/tailwind.css --minify",
    "watch:css": "npx @tailwindcss/cli -i ./src/input.css -o ./assets/css/tailwind.css --watch",
    "build": "npm run build:css && eleventy",
    "serve": "concurrently \"npm run watch:css\" \"eleventy --serve --watch\"",
    "clean": "rm -rf _site",
    "start": "npm run serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/font-awesome": "^1.0.1",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  }
}
